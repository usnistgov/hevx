{
  "configurations": [
    {
      "name": "x64-Debug",
      "generator": "Ninja",
      "configurationType": "Debug",
      "inheritEnvironments": [
        "msvc_x64_x64"
      ],
      "buildRoot": "${env.USERPROFILE}\\CMakeBuilds\\hevx\\build\\${name}",
      "installRoot": "${env.USERPROFILE}\\CMakeBuilds\\hevx\\install\\${name}",
      "cmakeCommandArgs": "-DBUILD_TESTING=OFF",
      "buildCommandArgs": "-v",
      "ctestCommandArgs": ""
    },
    {
      "name": "x64-Release",
      "generator": "Ninja",
      "configurationType": "Release",
      "inheritEnvironments": [
        "msvc_x64_x64"
      ],
      "buildRoot": "${env.USERPROFILE}\\CMakeBuilds\\hevx\\build\\${name}",
      "installRoot": "${env.USERPROFILE}\\CMakeBuilds\\hevx\\install\\${name}",
      "cmakeCommandArgs": "-DBUILD_TESTING=OFF",
      "buildCommandArgs": "-v",
      "ctestCommandArgs": ""
    },
    {
      "name": "Linux-Debug",
      "generator": "Ninja",
      "remoteMachineName": "${defaultRemoteMachineName}",
      "configurationType": "Debug",
      "remoteCMakeListsRoot": "/var/tmp/src/hevx/${name}",
      "cmakeExecutable": "/usr/bin/cmake3",
      "buildRoot": "${env.LOCALAPPDATA}\\CMakeBuilds\\hevx\\build\\${name}",
      "installRoot": "${env.LOCALAPPDATA}\\CMakeBuilds\\hevx\\install\\${name}",
      "remoteBuildRoot": "/var/tmp/build/hevx/build/${name}",
      "remoteInstallRoot": "/var/tmp/build/hevx/install/${name}",
      "remoteCopySources": true,
      "remoteCopySourcesMethod": "rsync",
      "remoteCopySourcesExclusionList": [".vs", ".git"],
      "rsyncCommandArgs" : "-t --delete --delete-excluded",
      "remoteCopyBuildOutput" : false,
      "cmakeCommandArgs": "-DBUILD_TESTING=OFF -DPYTHON_EXECUTABLE=/usr/bin/python3",
      "buildCommandArgs": "-v",
      "ctestCommandArgs": "",
      "inheritEnvironments": [ "linux_x64" ]
    },
    {
      "name": "Linux-Release",
      "generator": "Ninja",
      "remoteMachineName": "${defaultRemoteMachineName}",
      "configurationType": "RelWithDebInfo",
      "remoteCMakeListsRoot": "/var/tmp/src/hevx/${name}",
      "cmakeExecutable": "/usr/bin/cmake3",
      "buildRoot": "${env.LOCALAPPDATA}\\CMakeBuilds\\hevx\\build\\${name}",
      "installRoot": "${env.LOCALAPPDATA}\\CMakeBuilds\\hevx\\install\\${name}",
      "remoteBuildRoot": "/var/tmp/build/hevx/build/${name}",
      "remoteInstallRoot": "/var/tmp/build/hevx/install/${name}",
      "remoteCopySources": true,
      "remoteCopySourcesMethod": "rsync",
      "remoteCopySourcesExclusionList": [".vs", ".git"],
      "rsyncCommandArgs" : "-t --delete --delete-excluded",
      "remoteCopyBuildOutput" : false,
      "cmakeCommandArgs": "-DBUILD_TESTING=OFF -DPYTHON_EXECUTABLE=/usr/bin/python3",
      "buildCommandArgs": "-v",
      "ctestCommandArgs": "",
      "inheritEnvironments": [ "linux_x64" ]
    }
  ]
}